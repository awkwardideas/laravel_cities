{
    "name": "awkwardideas/laravel_cities",
    "description": "Seed all countries/cities from geonames.org database. Searchable DB tree, ready to use API & a bonus vue.js component!",
    "keywords": ["laravel", "geonames.org", "geolocation", "countries", "cities"],
    "license": "MIT",
    "homepage": "https://github.com/awkwardideas/laravel_cities.git",
    "authors": [
        {
            "name": "Giannis Gasteratos",
            "email": "igasteratos@gmail.com"
        },
		{
            "name": "Chad Haney",
            "email": "chad@awkwardideas.com"
        },
        {
            "name": "Awkward Ideas",
            "email": "support@awkwardideas.com"
        }
    ],	
    "require": {
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.4",
        "phpunit/phpunit": "^6.0",
        "symfony/console": "^3.2",
        "symfony/var-dumper": "^3.2",
        "monolog/monolog": "^1.22",
        "vlucas/phpdotenv": "^2.4",
		"awkwardideas/mypdo" : "dev-master"
    },     
    "autoload": {
        "psr-4" : {
            "Igaster\\LaravelCities\\"          : "src/",
            "Igaster\\LaravelCities\\Tests\\"   : "tests/"            
        }
    },
    "minimum-stability": "stable"
}
